          "Location Request"
        ],
        "summary": "Returns welcome message",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "int32",
                  "example": 200
                },
                "data": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "welcome"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
